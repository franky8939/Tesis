@article{de_la_macorra_dark_2002,
	title = {Dark {Group}: {Dark} {Energy} and {Dark} {Matter}},
	shorttitle = {Dark {Group}},
	url = {https://core.ac.uk/display/25364704?recSetID=},
	abstract = {We study the possibility that a dark group, a gauge group with particles interacting with the standard model particles only via gravity, is responsible for containing the dark energy and dark matter required by present day observations. We show that it is indeed possible and we determine the constrains for the dark group. The non-perturbative effects generated by a strong gauge coupling constant can de determined and a scalar potential for the dark meson fields is generated parameterizing the dark energy. On the other hand it is the massive particles, e.g. dark baryons, of the dark gauge group that give the corresponding dark matter. The mass of the dark particles is of the order of the condensation scale \${\textbackslash}Lambda\_c\$ and the temperature is 4-5 times smaller then the photon's temperature. The dark matter is of the warm matter type and it gives good fit to structure formation. The only parameters of the model are the number of particles of the dark group. The conditions to not introduce any fine tuning of the energy density at the condensation scale plus the CMB spectrum constrains the condensation scale to \$0.2 eV {\textless}{\textbackslash}Lambda\_c{\textless} 459 eV\$ and a inverse power potential with \$0.27{\textless}n{\textless}0.86\$. These values agree with the "unified dark gauge group" \$SU(3),N\_f=6, n=2/3\$ that has a warm dark matter with mass \$m=42 eV\$, a temperature \$T\_\{DM\}=T\_{\textbackslash}gamma/4.85\$ and a free streaming scale \${\textbackslash}lm\_\{fs\}=1.6 Mpc\$ with a contain mass \$M=4 {\textbackslash}times 10{\textasciicircum}\{11\} M\_{\textbackslash}diamond\$ (\$M\_{\textbackslash}diamond\$ is the solar mass). The dark energy has an equation of state parameter today \$w\_o=-0.91\$ and the model agrees well with the CMB data},
	language = {en-gb},
	urldate = {2020-02-23},
	author = {De la Macorra, A.},
	year = {2002},
	file = {Snapshot:/home/franky8939/snap/zotero-snap/common/Zotero/storage/MR54VLFL/25364704.html:text/html;Full Text PDF:/home/franky8939/snap/zotero-snap/common/Zotero/storage/QDMSNX3S/De la Macorra - 2002 - Dark Group Dark Energy and Dark Matter.pdf:application/pdf}
}

@article{arun_dark_2017,
	title = {Dark matter, dark energy, and alternate models: {A} review},
	volume = {60},
