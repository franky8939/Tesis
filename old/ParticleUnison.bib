	keywords = {Astrophysics - High Energy Astrophysical Phenomena},
	annote = {Comment: 6 pages, 2 figures. Submitted to proceedings of South African Institute of Physics conference 2018},
	file = {arXiv.org Snapshot:/home/franky8939/snap/zotero-snap/common/Zotero/storage/UGYHAA5C/1810.html:text/html}
}

