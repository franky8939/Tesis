@article{medina_enlarging_2017,
	title = {Enlarging {Regions} of the {MSSM} {Parameter} {Space} for {Large} \${\textbackslash}tan{\textbackslash}beta\$ via {SUSY} {Decays} of the {Heavy} {Higgs} {Bosons}},
	volume = {2017},
