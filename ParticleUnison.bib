@article{wells_upgraded_2015,
	title = {The upgraded {ATLAS} and {CMS} detectors and their physics capabilities},
	volume = {373},
	url = {https://royalsocietypublishing.org/doi/full/10.1098/rsta.2014.0046},
	doi = {10.1098/rsta.2014.0046},
	abstract = {The update of the European Strategy for Particle Physics from 2013 states that Europe's top priority should be the exploitation of the full potential of the LHC, including the high-luminosity upgrade of the machine and detectors with a view to collecting 10 times more data than in the initial design. The plans for upgrading the ATLAS and CMS detectors so as to maintain their performance and meet the challenges of increasing luminosity are presented here. A cornerstone of the physics programme is to measure the properties of the 125 GeV Higgs boson with the highest possible precision, to test its consistency with the Standard Model. The high-luminosity data will allow precise measurements of the dominant production and decay modes, and offer the possibility of observing rare modes including Higgs boson pair production. Direct and indirect searches for additional Higgs bosons beyond the Standard Model will also continue.},
	number = {2032},
	urldate = {2020-04-23},
	journal = {Philosophical Transactions of the Royal Society A: Mathematical, Physical and Engineering Sciences},
