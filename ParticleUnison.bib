@article{calmet_effective_2009,
	title = {Effective theory for dark matter and a new force in the dark matter sector},
	url = {https://core.ac.uk/display/81206386?recSetID=},
	doi = {10.1016/j.physletb.2009.07.049},
	abstract = {AbstractAn effective theory for dark matter has recently been proposed. The key assumption is that the dark matter particle which is a Dirac fermion is protected from decaying by a global U(1) symmetry. We point out that quantum gravity effects will violate this symmetry and that the dark matter candidate thus decays very fast. In order to solve that problem, we propose to consider a local gauge symmetry which implies a new force in the dark matter sector. It is likely that this new local U(1) symmetry will need to be spontaneously broken leading for a range of the parameters of the model to a Sommerfeld enhancement of the annihilation cross sections which is useful to explain the Pamela and ATIC results using a weakly interacting massive particle with a mass in the TeV range},
	language = {en-gb},
	urldate = {2020-02-22},
	author = {Calmet, Xavier and Majee, Swarup Kumar},
	year = {2009},
	file = {Full Text PDF:/home/franky8939/snap/zotero-snap/common/Zotero/storage/R5VL9S69/Calmet y Majee - 2009 - Effective Theory for Dark Matter and a New Force i.pdf:application/pdf;Snapshot:/home/franky8939/snap/zotero-snap/common/Zotero/storage/77JZYW4V/34194306.html:text/html;Snapshot:/home/franky8939/snap/zotero-snap/common/Zotero/storage/WIJWRQUZ/81206386.html:text/html;Full Text PDF:/home/franky8939/snap/zotero-snap/common/Zotero/storage/PZC4CMEC/Calmet y Majee - 2009 - Effective theory for dark matter and a new force i.pdf:application/pdf}
}

