}

@article{arnowitt_searching_1994,
	title = {Searching for {SUSY} dark matter},
	url = {https://core.ac.uk/display/25173123?recSetID=},
	abstract = {\{\vphantom{\}}{\textbackslash}tenrm The possibility of detecting supersymmetric dark matter is examined within the framework of the minimal supergravity model (MSGM) where the {\textbackslash}tilde\{Z\}\_\{1\} is the LSP for almost the entire parameter space. A brief discussion is given of experimental strategies for detecting dark matter. The relic density is constrained to obey 0.10 {\textbackslash}leq {\textbackslash}Omega\_\{{\textbackslash}tilde\{Z\}\_\{1\}\}h{\textasciicircum}2 {\textbackslash}leq0.35, consistent with COBE data. Expected event rates for an array of possible terrestrial detectors ({\textasciicircum}3He, CaF\_2, Ge, GaAs, NaI and Pb) are examined. In general, detectors relying on coherrent {\textbackslash}tilde\{Z\}\_\{1\}-nucleus scattering are more sensitive than detectors relying on incoherrent (spin-dependent) scattering. The dependence of the event rates as a function of the SUSY parameters are described. The detectors are generally most sensitive to the small m\_0 and small m\_\{{\textbackslash}tilde\{q\}\} and large tan{\textbackslash}beta part of the parameter space. The current b{\textbackslash}rightarrow s+{\textbackslash}gamma decay rate eliminates regions of large event rates for {\textbackslash}mu {\textgreater}0, but allows large event rates to still occur for {\textbackslash}mu{\textless}0. MSGM models that also possess SU(5)-type proton decay generally predict event rates below the expected sensitivity of current dark matter detectors.},
	language = {en-gb},
	urldate = {2020-02-23},
	author = {Arnowitt, Richard Lewis and Nath, Pran},
	year = {1994},
	file = {Snapshot:/home/franky8939/snap/zotero-snap/common/Zotero/storage/5P5NN2KC/25173123.html:text/html;Full Text PDF:/home/franky8939/snap/zotero-snap/common/Zotero/storage/8NEBJMVH/Arnowitt y Nath - 1994 - Searching for SUSY dark matter.pdf:application/pdf}
}

@article{arnowitt_sensitivity_1994,
	title = {Sensitivity of dark matter dectectors to {SUSY} dark matter},
	url = {https://core.ac.uk/display/25167889?recSetID=},
	abstract = {ABSTRACT The sensitivity of dark matter detectors to the lightest neutralino (\{{\textbackslash}tilde \{Z\}\_1\}) is considered within the framework of supergravity grand unification with radiative breaking of SU(2)xU(1). The relic density of the \{{\textbackslash}tilde \{Z\}\_1\} is constrained to obey 0.10 {\textbackslash}leq {\textbackslash}Omega\_\{{\textbackslash}tilde \{Z\}\_1\}h{\textasciicircum}2 {\textbackslash}leq 0.35, consistent with COBE data and current measurements of the Hubble constant. Detectors can be divided into two classes: those most sensitive to spin dependent incoherent scattering of the \{{\textbackslash}tilde \{Z\}\_1\} (e.g. CaF\_2) and those most sensitive to spin independent coherent scattering (high A nuclei e.g. Pb). The parameter space is studied over the range of 100GeV {\textbackslash}leq m\_0, m\_\{{\textbackslash}tilde \{g\}\} {\textbackslash}leq 1{\textasciitilde}TeV; 2 {\textbackslash}leq tan{\textbackslash}beta {\textbackslash}leq 20; and -2 {\textbackslash}leq A\_t/m\_0 {\textbackslash}leq 3 and it is found that the latter type detector is generally more sensitive than the former type. Thus at a sensitivity level of R {\textbackslash}geq 0.1 events/kg da, a lead detector could scan roughtly 30{\textbackslash}\% of the {\textasciitilde}parameter space studied, and an increase of {\textasciitilde}this sensitivity by a factor of 10 {\textasciitilde}would lead to coverage of about 70{\textbackslash}\% of the parameter space. Dark matter detectors are in general more sensitive to the high tan{\textbackslash}beta, low m\_\{{\textbackslash}tilde \{g\}\} and low m\_0 parts of the parameter space. The conditions of radiative breaking of SU(2)xU(1) enter importantly in analysing the efficiency of dark matter detectors},
	language = {en-gb},
	urldate = {2020-02-23},
	author = {Arnowitt, Richard Lewis and Nath, Pran},
	year = {1994},
	file = {Snapshot:/home/franky8939/snap/zotero-snap/common/Zotero/storage/MFIEGEZM/25167889.html:text/html;Full Text PDF:/home/franky8939/snap/zotero-snap/common/Zotero/storage/NYFBDBUX/Arnowitt y Nath - 1994 - Sensitivity of dark matter dectectors to SUSY dark.pdf:application/pdf}
}

