@article{cms_collaboration_search_2016,
	title = {Search for a very light {NMSSM} {Higgs} boson produced in decays of the 125 {GeV} scalar boson and decaying into tau leptons in pp collisions at sqrt(s) = 8 {TeV}},
	volume = {2016},
	issn = {1029-8479},
