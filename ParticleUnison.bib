	title = {Probing the properties of dark matter particles with astrophysical observations},
	url = {https://openaccess.leidenuniv.nl/handle/1887/82071},
	abstract = {It is an established fact that the Standard Model has to be extended to explain the so-called Beyond the Standard Model (BSM) phenomena: dark matter, matter-antimatter asymmetry of the Universe and neutrino flavour oscillations. The difficulty of direct detection of new particles lies in the huge parameter space of the possible candidates. Hence, data coming from the cosmological and astrophysical observations provide invaluable directions for laboratory experiments.In this thesis we explore two methods of constraining new-physics candidates: through their influence on the primordial nucleosynthesis and through observable differences in the matter distribution caused by free-streaming of the dark-matter particles. We concentrate on the well-motivated extension of the SM that aims at explaining all 3 BSM problems at the same time: the Neutrino Minimal Standard Model. In this extension, there are 3 additional heavy neutral leptons, one of which plays the role of dark matter, while the other two are necessary for induction of matter-antimatter asymmetry and neutrino oscillations. The dark-matter candidate is an example of a Warm Dark Matter particle, the free-streaming of which might be detected in the Lyman-α forest spectra of distant quasars. The other two particles have lifetimes that make them relevant to the primordial nucleosynthesis.},
	urldate = {2020-02-25},
	journal = {urn:isbn:9789085934257},
	author = {Magalich, A. and Science, Faculty of and Natuurkunde, Leiden Instituut Onderzoek},
	month = dec,
	year = {2019},
	file = {Magalich et al. - 2019 - Probing the properties of dark matter particles wi.pdf:/home/franky8939/snap/zotero-snap/common/Zotero/storage/9FGTBTUA/Magalich et al. - 2019 - Probing the properties of dark matter particles wi.pdf:application/pdf}
}

@article{beck_probing_2019,
	title = {Probing dark matter via neutrino-gamma-ray correlations},
	url = {http://arxiv.org/abs/1912.03117},
	abstract = {The nature of dark matter is one of the most pressing questions in modern cosmology. Much work has been focussed in the past upon probing potential particle dark matter via gamma-rays resulting from its annihilation or decay. These processs are dominated by the decay of pions and thus have associated neutrino fluxes. Despite this, neutrino observations have been poor in thir ability to constrain the properties of hypothetical dark matter particles due to a lack of sensitivity. Since the gamma-ray and neutrino emissions from WIMP dark matter are expected to be correlated it becomes possible to infer an associated neutrino flux to accompany any gamma-ray flux that might be attributed to dark matter. In this work we will show that it is possible to derive superior and novel constraints, particularly on leptophilic and high mass WIMP models, with this approach. This is particularly relevant in the face of leptonic-related excesses observed in both the worlds of particle and astrophysics.},
	urldate = {2020-02-25},
	journal = {arXiv:1912.03117 [astro-ph]},
	author = {Beck, Geoff},
	month = dec,
	year = {2019},
	note = {arXiv: 1912.03117},
	keywords = {Astrophysics - High Energy Astrophysical Phenomena},
	annote = {Comment: 7 pages, 4 figures, submitted to proceedings of HEASA 2019},
	file = {arXiv.org Snapshot:/home/franky8939/snap/zotero-snap/common/Zotero/storage/2NBDGLY8/1912.html:text/html;arXiv Fulltext PDF:/home/franky8939/snap/zotero-snap/common/Zotero/storage/UMCMX8C8/Beck - 2019 - Probing dark matter via neutrino-gamma-ray correla.pdf:application/pdf}
}

@article{ishiwata_probing_2020,
	title = {Probing heavy dark matter decays with multi-messenger astrophysical data},
	volume = {2020},
	issn = {1475-7516},
	url = {http://arxiv.org/abs/1907.11671},
	doi = {10.1088/1475-7516/2020/01/003},
	abstract = {We set conservative constraints on decaying dark matter particles with masses spanning a very wide range (\$10{\textasciicircum}4-10{\textasciicircum}\{16\}\$ GeV). For this we use multimessenger observations of cosmic-ray (CR) protons/antiprotons, electrons/positrons, neutrinos/antineutrinos and gamma rays. Focusing on decays into the \${\textbackslash}bar\{b\}b\$ channel, we simulate the spectra of dark matter yields by using the Dokshitzer-Gribov-Lipatov-Altarelli-Parisi equations and the \${\textbackslash}texttt\{Pythia\}\$ package. We then propagate the CRs of dark matter origin till Earth by using the state-of-the-art numerical frameworks \${\textbackslash}texttt\{CRPropa\}\$, \${\textbackslash}texttt\{GALPROP\}\$ and \${\textbackslash}texttt\{HelMod\}\$ for the solution of the CR transport equation in the extragalactic, Galactic region and the heliosphere, respectively. Conservative limits are obtained by requiring that the predicted dark matter spectra at Earth be less than the observed CR spectra. Overall, we exclude dark matter lifetimes of \$10{\textasciicircum}\{28\}\$ s or shorter for all the masses investigated in this work. The most stringent constraints reach \$10{\textasciicircum}\{30\}\$ s for very heavy dark matter particles with masses in the range \$10{\textasciicircum}\{11\}-10{\textasciicircum}\{14\}\$ GeV.},
	number = {01},
	urldate = {2020-02-25},
	journal = {Journal of Cosmology and Astroparticle Physics},
	author = {Ishiwata, Koji and Macias, Oscar and Ando, Shin'ichiro and Arimoto, Makoto},
	month = jan,
	year = {2020},
	note = {arXiv: 1907.11671},
	keywords = {High Energy Physics - Phenomenology, Astrophysics - Cosmology and Nongalactic Astrophysics, Astrophysics - High Energy Astrophysical Phenomena},
	pages = {003--003},
	annote = {Comment: 22 pages, 10 figures, V2: Minor changes to match JCAP published version},
	file = {arXiv.org Snapshot:/home/franky8939/snap/zotero-snap/common/Zotero/storage/QWK2AW6Z/1907.html:text/html;arXiv Fulltext PDF:/home/franky8939/snap/zotero-snap/common/Zotero/storage/YLVBQ2N9/Ishiwata et al. - 2020 - Probing heavy dark matter decays with multi-messen.pdf:application/pdf}
}

@article{corsetti_susy_2000,
	title = {{SUSY} {Dark} {Matter}},
	url = {https://core.ac.uk/display/25285956?recSetID=},
	abstract = {Several new effects have been investigated in recent analyses of supersymmetric dark matter. These include the effects of the uncertainties of wimp velocity distributions, of the uncertainties of quark densities, of large CP violating phases, of nonuniversalities of the soft SUSY breaking parameters at the unification scale and of coannihilation on supersymmetric dark matter. We review here some of these with emphasis on the effects of nonuniversalities of the gaugino masses at the unification scale on the neutralino-proton cross-section from scalar interactions. The review encompasses several models where gaugino mass nonuniversalities occur including SUGRA models and D brane models. One finds that gaugino mass nonuniversalities can increase the scalar cross-sections by as much as a factor of 10 and also significantly extend the allowed range of the neutralino mass consistent with constraints up to about 500 GeV. These results have important implications for the search for supersymmetric dark matter},
	language = {en-gb},
	urldate = {2020-02-23},
	author = {Corsetti, A. and Nath, Pran},
	year = {2000},
	keywords = {High Energy Physics - Phenomenology},
	annote = {Comment: Based on an invited talk at the conference Les Recontres de Physique de la Vallee d'Aoste "Results and Perspectives in Particle Physics", La Thuile, Italy, Feb. 27-March 4, 2000; 15 pages, Latex including 3 figures},
	file = {arXiv Fulltext PDF:/home/franky8939/snap/zotero-snap/common/Zotero/storage/TFI2F7I3/Corsetti y Nath - 2000 - SUSY Dark Matter.pdf:application/pdf;Snapshot:/home/franky8939/snap/zotero-snap/common/Zotero/storage/FC6VJ95G/25285956.html:text/html;arXiv.org Snapshot:/home/franky8939/snap/zotero-snap/common/Zotero/storage/2BIDJWE6/0005234.html:text/html;Snapshot:/home/franky8939/snap/zotero-snap/common/Zotero/storage/2VFMBJL2/25285956.html:text/html;Snapshot:/home/franky8939/snap/zotero-snap/common/Zotero/storage/ZVGB5V7X/2459239.html:text/html}
}

@article{arnowitt_dark_2003,
	title = {Dark {Matter}, {Muon} g-2 and {Other} {SUSY} {Constraints}},
	url = {https://core.ac.uk/display/2473291?recSetID=},
	abstract = {Recent developments constraining the SUSY parameter space are reviewed within
the framework of SUGRA GUT models. The WMAP data is seen to reduce the error in
the density of cold dark matter by about a factor of four, implying that the
lightest stau is only 5 -10 GeV heavier than the lightest neutralino when m\_0,
m\_\{1/2\} {\textless} 1 TeV. The CMD-2 re-analysis of their data has reduced the
disagreement between the Standard Model prediction and the Brookhaven
measurement of the muon magnetic moment to 1.9 sigma, while using the tau decay
data plus CVC, the disagreement is 0.7 sigma. (However, the two sets of data
remain inconsistent at the 2.9 sigma level.) The recent Belle and BABAR
measurements of the B -{\textgreater} phi K CP violating parameters and branching ratios are
discussed. They are analyzed theoretically within the BBNS improved
factorization method. The CP parameters are in disagreement with the Standard
Model at the 2.7 sigma level, and the branching ratios are low by a factor of
two or more over most of the parameter space. It is shown that both anomalies
can naturally be accounted for by adding a non-universal cubic soft breaking
term at M\_G mixing the second and third generations.Comment: 16 pages, 7 figures, plenary talk at Beyond The Desert '03, Castle
  Ringberg, Germany, June 9, 2003. Typos correcte},
	language = {en-gb},
	urldate = {2020-02-23},
	author = {Arnowitt, R. and Dutta, B. and Hu, B.},
	year = {2003},
	keywords = {High Energy Physics - Experiment, High Energy Physics - Phenomenology, High Energy Physics - Theory},
	annote = {Comment: 16 pages, 7 figures, plenary talk at Beyond The Desert '03, Castle Ringberg, Germany, June 9, 2003. Typos corrected},
	file = {arXiv Fulltext PDF:/home/franky8939/snap/zotero-snap/common/Zotero/storage/NKG6BEYN/Arnowitt et al. - 2003 - Dark Matter, Muon g-2 and Other SUSY Constraints.pdf:application/pdf;arXiv.org Snapshot:/home/franky8939/snap/zotero-snap/common/Zotero/storage/6XLI4VV4/0310103.html:text/html;Snapshot:/home/franky8939/snap/zotero-snap/common/Zotero/storage/N7KWHS96/2473291.html:text/html}
}

@article{arnowitt_dark_2002,
	title = {Dark {Matter} in {SUSY} {Models}},
	url = {https://core.ac.uk/display/25329413?recSetID=},
	abstract = {Direct detection experiments for neutralino dark matter in the Milky Way are examined within the framework of SUGRA models with R-parity invariance and grand unification at the GUT scale, M\_G. Models of this type apply to a large number of phenomena, and all existing bounds on the SUSY parameter space due to current experimental constraints are included. For models with universal soft breaking at M\_G (mSUGRA), the Higgs mass and b{\textbackslash}to s{\textbackslash}gamma constraints imply that the gaugino mass, m\_\{1/2\}, obeys m\_\{1/2\} {\textgreater}(300-400)GeV putting most of the parameter space in the co-annihilation domain where there is a relatively narrow band in the m\_0 - m\_\{1/2\} plane. For \${\textbackslash}mu {\textgreater} 0\$ we find that the neutralino -proton cross section {\textgreater}{\textasciitilde} 10{\textasciicircum}\{-10\} pb for m\_\{1/2\} {\textless} 1 TeV, making almost all of this parameter space accessible to future planned detectors. For {\textbackslash}mu {\textless} 0, however, there will be large regions of parameter space with cross sections {\textless} 10{\textasciicircum}\{-12\} pb, and hence unaccessible experimentally. If, however, the muon magnetic moment anomaly is confirmed, then {\textbackslash}mu {\textgreater}0 and m\_\{1/2\}{\textless}{\textasciitilde} 800 GeV. Models with non-universal soft breaking in the third generation and Higgs sector can allow for new effects arising from additional early universe annihilation through the Z-channel pole. Here cross sections that will be accessible in the near future to the next generation of detectors can arise, and can even rise to the large values implied by the DAMA data. Thus dark matter detectors have the possibility of studying the the post-GUT physics that control the patterns of soft breaking},
	language = {en-gb},
	urldate = {2020-02-23},
	author = {Arnowitt, Richard Lewis and Dutta, B. and Santoso, Y.},
	year = {2002},
	keywords = {High Energy Physics - Experiment, High Energy Physics - Phenomenology, Astrophysics},
	annote = {Comment: 13 pages, latex, 10 figures, invited talk at NANP-01, Dubna, Russia, June 19--23, 2001},
	file = {arXiv Fulltext PDF:/home/franky8939/snap/zotero-snap/common/Zotero/storage/WCUFRT7D/Arnowitt et al. - 2002 - Dark Matter in SUSY Models.pdf:application/pdf;arXiv.org Snapshot:/home/franky8939/snap/zotero-snap/common/Zotero/storage/G9KYXSNW/0201194.html:text/html;Snapshot:/home/franky8939/snap/zotero-snap/common/Zotero/storage/DKW8TYYZ/2466306.html:text/html;Snapshot:/home/franky8939/snap/zotero-snap/common/Zotero/storage/8UMVL5FE/25329413.html:text/html;Full Text PDF:/home/franky8939/snap/zotero-snap/common/Zotero/storage/CMZENCBG/Arnowitt et al. - 2002 - Dark Matter in SUSY Models.pdf:application/pdf}
}

@article{zacek_dark_2007,
	title = {Dark {Matter}},
	url = {https://core.ac.uk/display/1946954?recSetID=},
	doi = {10.1142/9789812776105_0007},
	abstract = {The nature of the main constituents of the mass of the universe is one of the
outstanding riddles of cosmology and astro-particle physics. Current models
explaining the evolution of the universe, and measurements of the various
components of its mass, all have in common that an appreciable contribution to
