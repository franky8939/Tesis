	keywords = {High Energy Physics - Experiment, High Energy Physics - Phenomenology, Astrophysics - High Energy Astrophysical Phenomena},
	annote = {Comment: 9 pages, 2 figures, 3 tables; v2: minor changes, added discussion of dominant annihilation final states, matches published version in PRD},
	file = {arXiv.org Snapshot:/home/franky8939/snap/zotero-snap/common/Zotero/storage/AMSDP3CF/1608.html:text/html;arXiv Fulltext PDF:/home/franky8939/snap/zotero-snap/common/Zotero/storage/65VUD7JU/Profumo y Stefaniak - 2016 - Alignment without Decoupling the Portal to Light .pdf:application/pdf}
