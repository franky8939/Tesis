	keywords = {High Energy Physics - Phenomenology, Astrophysics, High Energy Physics - Theory},
	pages = {391--419},
	annote = {Comment: 31 pages, 12 figures; v2: A couple of references added, a few minor clarifications (on KK parity and on the 6d case)},
	file = {arXiv.org Snapshot:/home/franky8939/snap/zotero-snap/common/Zotero/storage/986IZ9TA/0206071.html:text/html;arXiv Fulltext PDF:/home/franky8939/snap/zotero-snap/common/Zotero/storage/5XHHJ8PZ/Servant y Tait - 2003 - Is the Lightest Kaluza-Klein Particle a Viable Dar.pdf:application/pdf}
