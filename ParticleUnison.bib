@article{profumo_susy_2003,
	title = {{SUSY} {Dark} {Matter} and {Quintessence}},
	issn = {10.1088/1475-7516/2003/11/006},
	url = {https://core.ac.uk/display/2473061?recSetID=},
	doi = {10.1088/1475-7516/2003/11/006},
	abstract = {We investigate the enhancement of neutralino relic density in the context of
a realistic cosmological scenario with Quintessence. The accurate relic density
computation we perform allows us to be sensitive to both cases with shifts in
the abundance at the percent level, and to enhancements as large as 10{\textasciicircum}6. We
